{
  "name": "central-database",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "author": "Shadyzpop",
  "scripts": {
    "dev": "yarn build:dev && yarn start",
    "start": "node ./dist/index.js",
    "prod:start": "node ./index.js",
    "clean": "rm -rf ./dist",
    "build:prod": "tsc -p tsconfig.json && cp package.json dist/ && cp prod.env dist/.env && yarn schema:gen-schema && yarn schema:gen-types",
    "build:dev": "tsc -p tsconfig.json && cp package.json dist/ && cp .env dist/.env && yarn schema:gen-schema && yarn schema:gen-types",
    "schema:gen-types": "ts-node src/scripts/createTypes.ts",
    "schema:gen-schema": "ts-node src/scripts/transpileSchema.ts"
  },
  "dependencies": {
    "@gql2ts/from-schema": "^1.10.1",
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-favicon": "^2.0.1",
    "glob": "^7.1.3",
    "graphql": "^14.2.1",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.4",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.0",
    "mailgun-js": "^0.22.0",
    "merge-graphql-schemas": "^1.5.8",
    "mkdirp": "^0.5.1",
    "mongodb": "^3.2.3",
    "mongoose": "^5.7.5",
    "mongoose-autopopulate": "^0.9.1",
    "mongoose-relationship": "^0.1.5",
    "morgan": "^1.9.1",
    "otplib": "^11.0.0",
    "qrcode": "^1.3.3",
    "socket.io": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@gql2ts/types": "^1.9.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql": "^14.0.7",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mkdirp": "^0.5.2",
    "@types/mongoose": "^5.3.22",
    "@types/otplib": "^7.0.0",
    "@types/socket.io": "^2.1.2",
    "@types/uuid": "^3.4.4",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3"
  }
